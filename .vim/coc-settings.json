{
  "suggest.enablePreselect": true,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },

  "solargraph.checkGemVersion": false,
  "solargraph.hover": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true
}
